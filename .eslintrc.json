{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "max-len": ["error", { "code": 110 }],
      "no-underscore-dangle": ["off"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "no-confusing-arrow": ["error", {"allowParens": true}]
    },
    "parser": "babel-eslint"
}
